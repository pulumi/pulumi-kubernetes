{
  "entryPoints": ["index.ts"],
  "out": "docs",
  "exclude": [
    "**/node_modules/**",
    "**/tests/**",
    "**/*.test.ts"
  ],
  "excludePrivate": true,
  "excludeProtected": true,
  "excludeInternal": true,
  "excludeExternals": true,
  "readme": "README.md",
  "name": "@pulumi/kubernetes",
  "includeVersion": true,
  "sort": ["source-order"],
  "categorizeByGroup": true,
  "navigation": {
    "includeCategories": true,
    "includeGroups": true
  },
  "skipErrorChecking": true,
  "compilerOptions": {
    "skipLibCheck": true
  }
}
